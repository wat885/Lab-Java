package com.buzzfreeze.helloWorld;

public class lab4_1ControlFlow {

	
	public static void main(String [] args) {

		int score = 50;
		if(score >= 80) {
			System.out.println("Good");} 
		else if(score >= 50&& score < 80) {
			System.out.println("C normal");}
		else if(score <50){
			System.out.println("A fail");} 




		
	}
}
